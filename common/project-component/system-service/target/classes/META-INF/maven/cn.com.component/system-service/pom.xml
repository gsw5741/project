<?xml version="1.0"?>
<project
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd"
	xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>cn.com.component</groupId>
		<artifactId>project-component</artifactId>
		<version>0.0.1</version>
	</parent>
	<artifactId>system-service</artifactId>
	<packaging>jar</packaging>
	<dependencies>
	<dependency>
			<groupId>cn.com.component</groupId>
			<artifactId>system-cm</artifactId>
			<version>0.0.1</version>
			<exclusions>
				<exclusion>
					<artifactId>project-common</artifactId>
					<groupId>cn.com.project</groupId>
				</exclusion>
			</exclusions>
	</dependency>
		<dependency>
			<groupId>cn.com.component</groupId>
			<artifactId>project-spring</artifactId>
			<version>0.0.1</version>
			<exclusions>
				<exclusion>
					<groupId>cn.com.project</groupId>
					<artifactId>project-common</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		
		<dependency>
			<groupId>cn.com.component</groupId>
			<artifactId>project-shiro</artifactId>
			<version>0.0.1</version>
			<exclusions>
				<exclusion>
					<groupId>cn.com.project</groupId>
					<artifactId>project-common</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>cn.com.component</groupId>
			<artifactId>project-mybatis</artifactId>
			<version>0.0.1</version>
			<exclusions>
				<exclusion>
					<groupId>cn.com.project</groupId>
					<artifactId>project-common</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
	</dependencies>
	<build>
		<finalName>system-service</finalName>
		<pluginManagement>
			<plugins>
			
				<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.6.1</version>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>
			</plugins>
		</pluginManagement>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
				<includes>  
            		<include>mybatis/**</include>  
        		</includes>  
			</resource>
		</resources>
	</build>
</project>
